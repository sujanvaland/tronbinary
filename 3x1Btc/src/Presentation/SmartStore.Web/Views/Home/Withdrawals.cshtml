@model List<TransactionModel>
@using SmartStore.Admin.Models.Investment
@{
	ViewBag.Title = "Withdrawals";
	Layout = "~/Views/Shared/_PublicLayout.cshtml";
}



<br /><br /><br /><br /><br /><br /><br /><br /><br />
<div class="container">
	<div class="row">
		<div class="col-md-12">
			<h2>All Withdrawals</h2>
			<table id="tblReferral1" class="table table-striped table-hover table-bordered">
				<tr>
					<th>Transcation Id</th>
					<th>Date</th>
					<th>Name</th>
					<th>Amount</th>
					<th>Status</th>
				</tr>
				@foreach (var item in Model)
				{
					<tr>
						<td>@item.Id</td>
						<td>@item.TransactionDate</td>
						<td>@item.CustomerEmail</td>
						<td>$@item.FinalAmount</td>
						<td>@item.TransStatusString</td>
					</tr>
				}
			</table>
		</div>
	</div>
</div>
